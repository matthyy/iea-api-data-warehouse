{
  "name": "renewable-data-store-webservice",
  "version": "1.0.0",
  "description": "renewable IEA API",
  "scripts": {
    "prebuild": "rm -rf ./build/config",
    "import": "node data/import-data.js",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "lint": "eslint rc-df-af-coupon-webservice --ext .ts --quiet",
    "lint:fix": "eslint rc-df-af-coupon-webservice --ext .ts --fix",
    "test": "NODE_ENV=test jest",
    "test-ci": "NODE_ENV=test jest --clearCache && jest --coverage --silent --config jest.config.ci.js",
    "start": "node -r dotenv/config rc-df-af-coupon-webservice/build/server.js",
    "start:dev": "nodemon -r dotenv/config --watch 'src/**/*.ts' --exec 'ts-node --files' src/server.ts",
    "start:debug": "nodemon --watch 'src/**/*.ts' --exec 'node --inspect=0.0.0.0:9229 --require dotenv/config --require ts-node/register' src/server.ts"
  },
  "author": "iea",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.20",
    "@types/supertest": "^2.0.10",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "csvtojson": "^2.0.10",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "mongoose": "^5.11.12",
    "winston": "^3.3.3"
  }
}
